@model IEnumerable<VoterAnalysis2.Models.ElectionDayAssignment>

@{
    ViewData["Title"] = "SeeElectionDayAssignment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>SeeElectionDayAssignment</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Precinct)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StaffName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Precinct)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StaffName)
            </td>
            <td>
                <a asp-action="SeeVotersElectionDay" asp-route-id="@item.Id">See Voters</a>
                
            </td>
        </tr>
}
    </tbody>
</table>
